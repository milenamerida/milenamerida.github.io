    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="quantity" type="Integer" array="False" size=""/>
            <declare name="tax" type="Real" array="False" size=""/>
            <declare name="extprice" type="Real" array="False" size=""/>
            <declare name="total" type="Real" array="False" size=""/>
            <output expression="&quot;Enter the quantity of items &quot;" newline="True"/>
            <input variable="quantity"/>
            <if expression="quantity &gt;= 1000">
                <then>
                    <assign variable="extprice" expression="quantity*3"/>
                </then>
                <else>
                    <assign variable="extprice" expression="quantity*5"/>
                </else>
            </if>
            <assign variable="tax" expression="extprice*0.07"/>
            <assign variable="total" expression="extprice+tax"/>
            <output expression="&quot;Quantity &quot; &amp; quantity &amp; &quot; extended price $&quot; &amp; extprice &amp; &quot; tax $&quot; &amp; tax &amp; &quot; and total $&quot; &amp; total" newline="True"/>
        </body>
    </function>
